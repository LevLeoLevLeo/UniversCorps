<Page x:Class="UniversCorps.PagesCorp.PageCorpInformation"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UniversCorps.PagesCorp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageCorpInformation">

    <Grid>

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10 50 0 0">
 
            <TextBlock Text="Количество факультетов: " Style="{StaticResource TxtCore}"/>
            <TextBlock Name="TxtFuc" Style="{StaticResource TxtCore}" Margin="10 0 0 0"/>

        </StackPanel>

        <Button Name="BtnBack" Style="{StaticResource BtnMain}" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="120"
                Margin="10 0 0 10" Content="Назад" Click="BtnBack_Click"/>
        
    </Grid>

</Page>
